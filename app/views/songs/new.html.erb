<h1>Create Song</h1>

<%= form_for @song do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name%>
    <%= f.label :artist_id%>
    <%= f.collection_select :artist_id, Artist.all, :id, :name %>
    <%= f.label :genre_id%>
    <%= f.collection_select :genre_id, Genre.all, :id, :name %>
    <%= f.submit %>
<%end%>